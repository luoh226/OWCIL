{
    "prefix": "ccil",
    "dataset": "cifar100",
    "memory_size": 0,
    "shuffle": true,
    "init_cls": 10,
    "increment": 10,
    "model_name": "il2a",
    "convnet_type": "resnet18_cbam",
    "device": ["0"],
    "seed": [1993, 1999, 2024],
    "lambda_fkd":10,
    "lambda_proto":10,
    "temp":0.1,
    "epochs" : 101,
    "lr" : 0.001,
    "batch_size" : 64,
    "weight_decay" : 2e-4,
    "step_size":45,
    "gamma":0.1,
    "num_workers" : 2,
    "ratio": 2.5,
    "T" : 2,
    "auto_aug": false,

    "eval_only": false,
    "get_thresh": false,
    "k_fold": 3,
    "ood_ratio": 1.0,
    "ood_thresh_type": "TRP95",
    "ood_method": "MSP",

    "separate_head": false,
    "separate_type": 3,
    "b1": 0.92,
    "b2": 0.08,
    "loss_type":"mse",
    "activate_type": "relu",
    "a1": 0.5,
    "a2": 0.5,
    "coef_type": "exp",
    "coef_start": 0.01
}