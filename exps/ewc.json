{
    "prefix": "ccil",
    "dataset": "cifar100",
    "memory_size": 0,
    "memory_per_class": 20,
    "fixed_memory": false,
    "shuffle": true,
    "init_cls": 10,
    "increment": 10,
    "model_name": "ewc",
    "convnet_type": "resnet18",
    "device": ["0"],
    "seed": [1993, 1999, 2024],
    "num_workers" : 2,
    "auto_aug": false,

    "eval_only": false,
    "get_thresh": false,
    "k_fold": 3,
    "ood_ratio": 1.0,
    "ood_thresh_type": "TRP95",
    "ood_method": "MSP",

    "separate_head": false,
    "separate_type": 3,
    "b1": 0.92,
    "b2": 0.08,
    "loss_type":"mse",
    "activate_type": "relu",
    "a1": 0.5,
    "a2": 0.5,
    "coef_type": "exp",
    "coef_start": 0.01
}