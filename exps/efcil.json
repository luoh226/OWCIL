{
    "prefix": "ccil",
    "dataset": "cifar100",
    "memory_size": 0,
    "shuffle": true,
    "init_cls": 10,
    "increment": 10,
    "model_name": "efcil",
    "convnet_type": "resnet18",
    "device": ["0"],
    "seed": [1993],
    "batch_size": 128,

    "loss_type":"mse",
