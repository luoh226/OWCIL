    "separate_head": true,
    "separate_type": 3,
    "b1": 0.92,
    "b2": 0.08,
    "auto_aug": false,

    "eval_ccil": true,
    "ood_method": "EDL6",
    "ood_thresh": 1.0,
    "eval_only": false
}
